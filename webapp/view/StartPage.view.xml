<!-- This message page is shown when the user has not entered anything yet. -->
<!-- Because there is no interaction on the page, no controller is needed -->
<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
		<MessagePage description="" icon="sap-icon://person-placeholder" id="startPage" showHeader="false"
		text="{i18n>ymsg.initialMessage}" title="{i18n>xtit.mainPageTitle}" ></MessagePage>
</mvc:View>